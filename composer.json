{
  "name": "terminus-plugin-project/terminus-s3-sync-plugin",
  "description": "Terminus plugin to sync Pantheon backups with S3 buckets",
  "authors": [
      {
          "name": "Sean Dietrich",
          "email": "sean.e.dietrich@gmail.com"
      }
  ],
  "minimum-stability": "dev",
  "type": "terminus-plugin",
  "require": {
    "php": ">=5.4",
    "aws/aws-sdk-php": "^3.21",
    "magicalex/write-ini-file": "^1.2",
    "react/stream": "^0.4.6",
    "react/event-loop": "^0.4.2",
    "react/promise": "^2.5",
    "clue/stream-filter": "^1.3"
  },
  "autoload": {
    "psr-4": { "Pantheon\\TerminusS3Sync\\": "src" }
  },
  "extra": {
    "terminus": {
      "compatible-version": "^1"
    }
  }
}
